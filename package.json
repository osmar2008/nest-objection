    "@nestjs/core": "8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "8.0.0",
    "class-validator": "^0.14.0",
    "dotenv": "^10.0.0",
    "knex": "^0.95.15",
    "objection": "^3.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.15",
    "eslint": "^6.1.3",
