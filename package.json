  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "test": "jest",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main.js",
    "knex": "knex --knexfile knexfile.ts",
